	<tag>
		<name>pathSplitter</name>
		<tagclass>com.actuate.reportcast.tags.common.PathSplitterTag</tagclass>
		<teiclass>com.actuate.reportcast.tags.common.PathSplitterTEI</teiclass>
		<bodycontent>JSP</bodycontent>
		<info>
			A tag that may be used to split an encyclopedia path 
			into individual components
		</info>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>name</name>
